{
	"name": "get-exif",
	"version": "0.1.0",
	"description": "Get Exif data from JPEG or TIFF data",
	"repository": "shinnn/get-exif",
	"author": "Shinnosuke Watanabe (https://github.com/shinnn)",
	"scripts": {
		"pretest": "eslint --format=codeframe .",
		"test": "nyc --reporter=html --reporter=text node test.js"
	},
	"license": "ISC",
	"files": [
		"index.js"
	],
	"keywords": [
		"exif",
		"metadata",
		"photo",
		"extract",
		"parse",
		"buffer",
		"jpg",
		"jpeg",
		"tif",
		"tiff",
		"image"
	],
	"dependencies": {
		"is-jpg": "^1.0.1",
		"is-tif": "^1.0.1",
		"piexifjs": "^1.0.4",
		"safe-buffer": "^5.1.2"
	},
	"devDependencies": {
		"@shinnn/eslint-config-node": "^5.0.0",
		"eslint": "^5.0.1",
		"nyc": "^12.0.2",
		"smallest-jpeg": "^1.0.1",
		"tape": "^4.9.1"
	},
	"eslintConfig": {
		"extends": "@shinnn/node",
		"rules": {
			"no-var": "off",
			"prefer-template": "off"
		}
	}
}
